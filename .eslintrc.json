{
  "installedESLint": true,
  "extends": [
    "airbnb", "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "indent": [1, 2, {"SwitchCase": 1}],
    "react/prefer-stateless-function": 0,
    "no-unused-vars": ["error", { "varsIgnorePattern": "_*", "ignoreRestSiblings": true}]
  }
}

